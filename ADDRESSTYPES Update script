update WASADMIN.ADDRESSTYPES set DESCRIPTION = 'PHYSICAL ADDRRESS' where ADDRESSTYPE = 1;
update WASADMIN.ADDRESSTYPES set DESCRIPTION = 'P.O. BOX ADDRESS' where ADDRESSTYPE = 2;
update WASADMIN.ADDRESSTYPES set DESCRIPTION = 'CORRESPONDENCE ADDRESS' where ADDRESSTYPE = 3;
update WASADMIN.ADDRESSTYPES set DESCRIPTION = 'PREVIOUS ADDRESS 0' where ADDRESSTYPE = 4;
update WASADMIN.ADDRESSTYPES set DESCRIPTION = 'PREVIOUS ADDRESS 1' where ADDRESSTYPE = 5;
update WASADMIN.ADDRESSTYPES set DESCRIPTION = 'ALTERNATIVE ADDRESS' where ADDRESSTYPE = 6;
commit;

		

